<Window x:Class="AccountsManager.SetMasterPasswordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AccountsManager"
        mc:Ignorable="d"
        Title="SetMasterPasswordWindow" Height="300" Width="399" Closed="Window_Closed" >
    <Grid HorizontalAlignment="Left" Margin="0,0,-0.4,0.4" Width="294">
        <TextBox x:Name="txtBoxPassword" HorizontalAlignment="Left" Height="23" Margin="84,94,-46,0" TextWrapping="Wrap" Text="Enter Password Here..." VerticalAlignment="Top" Width="256"/>
        <Button Content="Set Password" HorizontalAlignment="Left" Margin="170,137,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

    </Grid>
</Window>
